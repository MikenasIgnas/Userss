{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "import/prefer-default-export": "off",
    "import/no-default-export": "error",
    "import/extensions": [ "error", "ignorePackages", { "js": "always"} ],
   
    "max-len": ["error", {"code":200 }],
    "array-callback-return": [0]
  }
}
